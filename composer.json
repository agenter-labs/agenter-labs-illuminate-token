{
    "name": "agenter-labs/illuminate-token",
    "description": "Token manager Laravel",
    "keywords": [
        "laravel",
        "token"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/encryption": "^8.0",
        "illuminate/cache": "^8.0",
        "agenter-labs/illuminate-uid": "^0.0.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.15",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
    "psr-4": {
            "AgenterLab\\Token\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\Token\\TokenServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
